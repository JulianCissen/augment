{
  "name": "hello-world-plugin",
  "version": "1.0.0",
  "description": "A simple hello world plugin example for Augment",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "build": "augment-builder build -i ./src -o ./dist",
