{
  "name": "@augment/boilerplate",
  "version": "1.0.0",
  "description": "Official boilerplate template for creating Augment plugins",
  "keywords": [
    "augment",
    "plugin",
    "template",
    "boilerplate"
  ],
  "license": "MIT",
  "author": "Julian Cissen",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "augment-builder build -i ./src -o ./dist",
    "build:zip": "augment-builder build -i ./src -o ./dist --zip",
    "dev": "augment-builder watch -i ./src -o ./dist"
  },
  "devDependencies": {
    "@augment/builder": "^0.1.0"
  },
  "peerDependencies": {
    "@augment/core": "^0.1.0"
  }
}
